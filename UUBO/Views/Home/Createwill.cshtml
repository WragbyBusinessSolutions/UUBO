<script type="text/javascript">
    $("body").on("click", "#btnAdd", function () {
        //Reference the Name and Country TextBoxes.
        var txtFullName = $("#txtFullName");
        var txtDate = $("#txtDate");
        var txtSpecIns = $("#txtSpecIns");
        var txtAssetExc = $("#txtAssetExc");

        console.log(txtFullName.val(), txtDate.val(), txtSpecIns.val(), txtAssetExc.val());
        //Get the reference of the Table's TBODY element.
        var tBody = $("#tblChild > TBODY")[0];
        //Add Row.
        var row = tBody.insertRow(-1);

        //Add Name cell.
        var cell = $(row.insertCell(-1));
        cell.html(txtFullName.val());

        //Add Date cell.
        cell = $(row.insertCell(-1));
        cell.html(txtDate.val());

        //Add Special Instruction cell.
        cell = $(row.insertCell(-1));
        cell.html(txtSpecIns.val());

        //Add Asset Exclusion cell.
        cell = $(row.insertCell(-1));
        cell.html(txtAssetExc.val());

        //Add Button cell.
